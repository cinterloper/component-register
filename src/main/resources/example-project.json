{
  "name": "example-project",
  "env_vars": ["KEYSTORE_PASS"],
  "output_type":"json",
  "default_launch_strategy":"waiting",
  "kvdn_port":6500,
  "kvdn_route_provider":"net.iowntheinter.util.http.example.helloWorldRouteProvider",
  "reflectors":{
    "_KVDN_+cornerstone:registration":"_cornerstone:registration"
  },
  "injectors":{
    "JWTInjector":"net.iowntheinter.util.crypto.JWTInjector"
  },
  "crypto":{
    "jwt":{
      "path":"keystore.jceks",
      "type":"jceks"
    }
  },
  "startup": {
    "vx": {
      "net.iowntheinter.util.ebReflector": {
        "deps": [],
        "enabled": true
      },
      "groovy:example.groovy": {
        "deps": [],
        "enabled": true
      }

    },
    "ext": {
      "docker": {
        "ubuntu": {
          "launchStrategy":"coordinated",
          "ifExists": "recreate",
          "dkrOptsRef": "testDkrOpts",
          "image": "shadowsystem/baseimage",
          "tag": "latest",
          "deps": [],
          "environment_injectors":["JWTInjector"],
          "enabled": true
        }
      }
    }
  },
  "optionBlocks": {
    "testDkrOpts": {
      "ExposedPorts": {
        "4711/tcp": {}
      },
      "HostConfig": {
        "PortBindings": {
        }
      }
    }
  }
}
