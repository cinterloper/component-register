{
  "name": "example-project",
  "env_vars": ["JCEKS_KEYSTORE_PASS"],
  "output_type":"json",
  "default_launch_strategy":"independent",
  "launch_stratagies":{
    "independent": null,
    "group":       "net.iowntheinter.coreLauncher.impl.startSignalLaunchStrategy",
    "capabilities":"net.iowntheinter.coreLauncher.impl.capabilitiesLaunchStrategy"
  },

  "kvdn":{
    "port":6500,
    "route_providers":["net.iowntheinter.util.http.example.helloWorldRouteProvider"]
  },
  "reflectors":{
    "_KVDN_+cornerstone:registration":"_cornerstone:registration"
  },
  "injectors":{
    "JWTInjector":"net.iowntheinter.util.crypto.JWTInjector"
  },
  "crypto":{
    "jwt":{
      "pass":"$$JCEKS_KEYSTORE_PASS",
      "path":"keystore.jceks",
      "type":"jceks"
    }
  },
  "startup": {
    "vx": {
      "net.iowntheinter.util.ebReflector": {
        "deps": [],
        "enabled": true
      },
      "groovy:example.groovy": {
        "deps": [],
        "count":2,
        "enabled": true,
        "worker":false
      }

    },
    "ext": {
      "docker": {
        "ubuntu": {
          "launchStrategy":"independent",
          "ifExists": "recreate",
          "dkrOptsRef": "testDkrOpts",
          "image": "ubuntu",
          "tag": "latest",
          "deps": [],
          "environment_injectors":["JWTInjector"],
          "enabled": false
        },
        "ubuntu2": {
          "launchStrategy":"group",
          "ifExists": "recreate",
          "dkrOptsRef": "testDkrOpts",
          "image": "ubuntu",
          "tag": "latest",
          "deps": [],
          "environment_injectors":["JWTInjector"],
          "enabled": false
        }
      }
    }
  },
  "optionBlocks": {
    "testDkrOpts": {
      "ExposedPorts": {
        "4711/tcp": {}
      },
      "Cmd": [
        "sleep",
        "360000"
      ],
      "HostConfig": {
        "PortBindings": {
        }
      }
    }
  }
}
