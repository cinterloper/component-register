{
  "name": "example-project",
  "env_vars": [],
  "startup": {
    "vx": {
      "groovy:example.groovy": {
        "deps": [],
        "startReady": true
      }
    },
    "ext": {
      "docker": {
        "zookeeper": {
          "ifExists": "recreate",
          "dkrOptsRef": "zkOpts",
          "image": "jplock/zookeeper",
          "tag": "latest",
          "deps": [],
          "startReady": true
        },
        "ctrName4": {
          "ifExists": "recreate",
          "dkrOptsRef": "testDkrOpts",
          "image": "ubuntu",
          "tag": "latest",
          "deps": [],
          "startReady": true
        }
      }
    }
  },
  "optionBlocks": {
    "testDkrOpts": {
      "Cmd": [
        "sh",
        "-c",
        "ping 127.0.0.1"
      ],
      "ExposedPorts": {
        "4711/tcp": {}
      },
      "HostConfig": {
        "PortBindings": {
        }
      }
    },
    "zkOpts": {
      "Cmd":  ["start-foreground"],
      "ExposedPorts": {
        "2181/tcp": {},
        "2888/tcp": {},
        "3888/tcp": {}
      },
      "HostConfig": {
        "PortBindings": {
          "2181/tcp": [{ "HostPort": "2181" }],
          "2888/tcp": [{ "HostPort": "2888" }],
          "3888/tcp": [{ "HostPort": "3888" }]
        }
      }
    }
  }
}