{
  "name": "example-project",
  "env_vars": [],
  "output_type":"table",
  "kvdn_port":6500,
  "kvdn_route_provider":"net.iowntheinter.kvdn.http.example.helloWorldRouteProvider",
  "reflectors":{
    "_KVDN_+cornerstone:registration":"_cornerstone:registration"
  },
  "startup": {
    "vx": {
      "net.iowntheinter.util.ebReflector": {
        "deps": [],
        "enabled": true
      },
      "groovy:example.groovy": {
        "deps": [],
        "enabled": true
      }

    },
    "ext": {
      "docker": {
        "ubuntu": {
          "ifExists": "recreate",
          "dkrOptsRef": "zkOpts",
          "image": "jplock/zookeeper",
          "tag": "latest",
          "deps": [],
          "enabled": false
        },
        "ctrName4": {
          "ifExists": "recreate",
          "dkrOptsRef": "testDkrOpts",
          "image": "ubuntu",
          "tag": "latest",
          "deps": [],
          "enabled": false
        }
      }
    }
  },
  "optionBlocks": {
    "testDkrOpts": {
      "Cmd": [
        "sh",
        "-c",
        "ping 127.0.0.1"
      ],
      "ExposedPorts": {
        "4711/tcp": {}
      },
      "HostConfig": {
        "PortBindings": {
        }
      }
    },
    "zkOpts": {
      "Cmd":  ["start-foreground"],
      "Env":["THIS=123"],
      "ExposedPorts": {
        "2181/tcp": {},
        "2888/tcp": {},
        "3888/tcp": {}
      },
      "HostConfig": {
        "PortBindings": {
          "2181/tcp": [{ "HostPort": "2181" }],
          "2888/tcp": [{ "HostPort": "2888" }],
          "3888/tcp": [{ "HostPort": "3888" }]
        }
      }
    }
  }
}
