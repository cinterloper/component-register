{
  "launchgrp": [
    {
      "type": "verticle",
      "name": "example1.groovy",
      "opts": {
        "dependencies": [
          "example2.groovy"
        ],
        "worker": true
      },
      "startReady": false
    },
    {
      "type": "verticle",
      "instances": 1,
      "name": "example2.groovy",
      "opts": {
        "dependencies": null
      },
      "startReady": true
    },
    {
      "type": "xetask",
      "name": "sleep",
      "opts": {
        "dependencies": null,
        "vars": {
          "TIME": 100
        }
      },
      "startReady": true
    },
    {
      "type": "docker",
      "image": "shadowhosting/baseimage",
      "name": "docker_launch_test",
      "opts": {
        "dependencies": null,
        "volumes": {
          "tstdata": "/tmp/tst"
        },
        "binds": {
          "/svr/path": "/ctr/path"
        }
      }
    }
  ]
}


