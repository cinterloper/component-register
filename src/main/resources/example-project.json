{
  "name": "example-project",
  "env_vars": [],
  "output_type":"json",
  "kvdn_port":6500,
  "kvdn_route_provider":"net.iowntheinter.util.http.example.helloWorldRouteProvider",
  "reflectors":{
    "_KVDN_+cornerstone:registration":"_cornerstone:registration"
  },
  "injectors":{
    "JWTInjector":"net.iowntheinter.util.crypto.JWTInjector"
  },
  "startup": {
    "vx": {
      "net.iowntheinter.util.ebReflector": {
        "deps": [],
        "enabled": true
      },
      "groovy:example.groovy": {
        "deps": [],
        "enabled": true
      }

    },
    "ext": {
      "docker": {
        "ubuntu": {
          "launchStrategy":"waiting",
          "ifExists": "recreate",
          "dkrOptsRef": "testDkrOpts",
          "image": "shadowsystem/baseiimage",
          "tag": "latest",
          "deps": [],
          "environment_injectors":["JWTInjector"],
          "enabled": true
        }
      }
    }
  },
  "optionBlocks": {
    "testDkrOpts": {
      "ExposedPorts": {
        "4711/tcp": {}
      },
      "HostConfig": {
        "PortBindings": {
        }
      }
    }
  }
}
